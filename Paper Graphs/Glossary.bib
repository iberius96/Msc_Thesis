	language = {en},
	month = jun,
	pages = {1--45},
	title = {Comparison of text preprocessing methods},
	url = {https://www.cambridge.org/core/product/identifier/S1351324922000213/type/journal_article},
	urldate = {2022-11-25},
	year = {2022},
	bdsk-url-1 = {https://www.cambridge.org/core/product/identifier/S1351324922000213/type/journal_article},
