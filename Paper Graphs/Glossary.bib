	journal = {International Journal of Hospitality Management},
	keywords = {eWOM, Hotel attributes, Performance, Behaviors},
	pages = {9-18},
	title = {The effects of hotel attribute performance on electronic word-of-mouth (eWOM) behaviors},
	url = {https://www.sciencedirect.com/science/article/pii/S0278431917307272},
