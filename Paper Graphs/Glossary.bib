	journal = {Information Systems Frontiers},
	language = {en},
	month = dec,
	shorttitle = {Bringing {Smart} {Home} {Technology} to {Peer}-to-{Peer} {Accommodation}},
	title = {Bringing {Smart} {Home} {Technology} to {Peer}-to-{Peer} {Accommodation}: {Exploring} the {Drivers} of {Intention} to {Stay} in {Smart} {Accommodation}},
	url = {https://link.springer.com/10.1007/s10796-021-10227-4},
