	doi = {10.1016/j.aci.2019.11.003},
	file = {Full Text:files/1811/Alqaryouti et al. - 2020 - Aspect-based sentiment analysis using smart govern.pdf:application/pdf},
	issn = {2634-1964, 2210-8327},
