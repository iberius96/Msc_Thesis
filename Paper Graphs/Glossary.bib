	issn = {1664-1078},
	journal = {Frontiers in Psychology},
	month = nov,
	pages = {776999},
	shorttitle = {Judgments in the {Sharing} {Economy}},
	title = {Judgments in the {Sharing} {Economy}: {The} {Effect} of {User}-{Generated} {Trust} and {Reputation} {Information} on {Decision}-{Making} {Accuracy} and {Bias}},
	url = {https://www.frontiersin.org/articles/10.3389/fpsyg.2021.776999/full},
	urldate = {2022-11-25},
	volume = {12},
