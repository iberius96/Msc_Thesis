	issn = {0138-9130, 1588-2861},
	journal = {Scientometrics},
	language = {en},
	month = apr,
	number = {4},
	pages = {1801--1823},
	shorttitle = {Selecting the appropriate leading journal in {Hospitality} and {Tourism} research},
