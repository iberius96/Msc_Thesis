	doi = {10.48550/ARXIV.2207.02160},
	keywords = {FOS: Computer and information sciences, Computation and Language (cs.CL), Information Retrieval (cs.IR), Artificial Intelligence (cs.AI)},
	note = {Publisher: arXiv Version Number: 1},
	title = {A {Comprehensive} {Review} of {Visual}-{Textual} {Sentiment} {Analysis} from {Social} {Media} {Networks}},
