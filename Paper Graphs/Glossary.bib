	journal = {International Journal of Advanced Trends in Computer Science and Engineering},
	month = apr,
	number = {2},
	pages = {1284--1288},
	title = {An {Analysis} on {Text} {Mining} {Techniques} for {Smart} {Literature} {Review}},
