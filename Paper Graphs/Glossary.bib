	journal = {Aslib Journal of Information Management},
	language = {en},
	month = sep,
	title = {Discovery of topic evolution path and semantic relationship based on patent entity representation},
	url = {https://www.emerald.com/insight/content/doi/10.1108/AJIM-03-2022-0124/full/html},
	urldate = {2022-11-25},
