	issn = {2053-1680, 2053-1680},
	journal = {Research \& Politics},
	language = {en},
	month = apr,
	number = {2},
	pages = {205316802110222},
	shorttitle = {Transfer learning for topic labeling},
	title = {Transfer learning for topic labeling: {Analysis} of the {UK} {House} of {Commons} speeches 1935--2014},
