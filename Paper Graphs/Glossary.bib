	doi = {10.18653/v1/2020.acl-main.32},
	month = jul,
	pages = {340--350},
	publisher = {Association for Computational Linguistics},
	title = {Neural Topic Modeling with Bidirectional Adversarial Training},
	url = {https://aclanthology.org/2020.acl-main.32},
	year = {2020},
