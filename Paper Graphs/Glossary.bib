	shorttitle = {A {Perspective} of {Electronic} {Cigarette} {Users} on the {COVID}-19 {Pandemic} in {Twitter}},
	title = {A {Perspective} of {Electronic} {Cigarette} {Users} on the {COVID}-19 {Pandemic} in {Twitter}: {Observational} {Study} ({Preprint})},
