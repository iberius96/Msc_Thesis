	issn = {0957-4174},
	journal = {Expert Systems with Applications},
	keywords = {Review rating prediction, Unbiased rating prediction, Deep learning, CNN, Opinion Mining},
	pages = {115111},
	title = {Discrepancy detection between actual user reviews and numeric ratings of Google App store using deep learning},
	url = {https://www.sciencedirect.com/science/article/pii/S0957417421005522},
	volume = {181},
	year = {2021},
