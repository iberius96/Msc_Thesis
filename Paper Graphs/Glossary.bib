	issn = {2374-3468, 2159-5399},
	journal = {Proceedings of the AAAI Conference on Artificial Intelligence},
	month = apr,
	number = {01},
	pages = {370--377},
	title = {Tracking {Disaster} {Footprints} with {Social} {Streaming} {Data}},
	url = {https://ojs.aaai.org/index.php/AAAI/article/view/5372},
	urldate = {2022-11-25},
	volume = {34},
	year = {2020},
	bdsk-url-1 = {https://ojs.aaai.org/index.php/AAAI/article/view/5372},
