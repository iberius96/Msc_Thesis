	journal = {Tourism Management},
	keywords = {Tourism branding, Destination marketing, Tourism communications, Personification, Anthropomorphic tendency},
	pages = {65-75},
	title = {Effects of personification and anthropomorphic tendency on destination attitude and travel intentions},
	url = {https://www.sciencedirect.com/science/article/pii/S0261517717300729},
