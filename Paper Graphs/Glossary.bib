	month = mar,
	number = {3},
	pages = {5015--5025},
	shorttitle = {Additive {DenseNet}},
	title = {Additive {DenseNet}: {Dense} connections based on simple addition operations},
	url = {https://www.medra.org/servlet/aliasResolver?alias=iospress&doi=10.3233/JIFS-201758},
	urldate = {2022-11-25},
