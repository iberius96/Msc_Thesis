	journal = {International Journal of Hospitality Management},
	keywords = {Sharing economy, Trust building model, Cognitive-based trust, Affective-based trust, Attachment theory, Identity-based attachment, Bond-based attachment, Airbnb},
	pages = {198-209},
	title = {In Airbnb we trust: Understanding consumers' trust-attachment building mechanisms in the sharing economy},
	url = {https://www.sciencedirect.com/science/article/pii/S027843191830327X},
	volume = {83},
