	doi = {https://doi.org/10.1016/j.infsof.2017.06.002},
	issn = {0950-5849},
	journal = {Information and Software Technology},
	keywords = {Software effort estimation, Latent Dirichlet allocation, Research trends},
	pages = {1-21},
	title = {Research patterns and trends in software effort estimation},
