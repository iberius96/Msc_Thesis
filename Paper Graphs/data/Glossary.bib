	doi = {song_2022_automatic_phenotyping_by_a_seed_guided_topic_model},
	isbn = {9781450393850},
	keywords = {variational autoencoder, predictive healthcare, topic modeling, electronic health records},
	location = {Washington DC, USA},
	numpages = {11},
