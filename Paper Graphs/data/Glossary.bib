	issn = {0278-4319},
	journal = {International Journal of Hospitality Management},
	keywords = {Peer-to-peer interactions, Self-disclosure, Reciprocity, Guest, Host, Airbnb},
	pages = {405-414},
	title = {Peer-to-peer interactions: Perspectives of Airbnb guests and hosts},
	url = {https://www.sciencedirect.com/science/article/pii/S0278431917305819},
	volume = {77},
