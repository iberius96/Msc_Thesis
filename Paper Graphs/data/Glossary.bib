	issn = {0261-5177},
	journal = {Tourism Management},
	keywords = {Halal tourism, Social environment, Locals and staff, Facilities, Muslim travelers},
	pages = {151-164},
	title = {Exploring halal-friendly destination attributes in South Korea: Perceptions and behaviors of Muslim travelers toward a non-Muslim destination},
	url = {https://www.sciencedirect.com/science/article/pii/S0261517718302450},
