	journal = {International Journal of Information Management},
	keywords = {Customer satisfaction, Customer dissatisfaction, Online textual reviews, Text mining, Regression},
	number = {6},
	pages = {673-683},
	title = {Business intelligence in online customer textual reviews: Understanding consumer perceptions and influential factors},
	url = {https://www.sciencedirect.com/science/article/pii/S0268401217301378},
	volume = {37},
	year = {2017},
