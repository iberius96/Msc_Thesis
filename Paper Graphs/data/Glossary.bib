	journal = {Scientometrics},
	number = {2},
	pages = {869--892},
	title = {Emerging trends and new developments in information science: a document co-citation analysis (2009--2016)},
	url = {https://doi.org/10.1007/s11192-018-2695-9},
	volume = {115},
	year = {2018},
