	keywords = {Destination choice, Discrete choice model, Reference-dependent behavior, Reference-level bias, Long-haul leisure travel},
	pages = {58-74},
	title = {Modeling reference experience in destination choice},
	url = {https://www.sciencedirect.com/science/article/pii/S016073831830063X},
	volume = {72},
