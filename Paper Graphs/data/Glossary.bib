	journal = {Journal of Retailing and Consumer Services},
	keywords = {Cross-cultural, Mall shoppers, Hofstede's cultural dimensions, Purchasing power parity},
	pages = {101951},
	title = {The role of culture and purchasing power parity in shaping mall-shoppers' profiles},
	url = {https://www.sciencedirect.com/science/article/pii/S0969698919300050},
	volume = {52},
	year = {2020},
