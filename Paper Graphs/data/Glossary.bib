	issn = {0160-7383},
	journal = {Annals of Tourism Research},
	keywords = {Destination choice, Destination brand equity, Dream destination, Favourite destination, Destination image, Destination imagery},
	pages = {81-94},
	title = {A destination imagery processing model: Structural differences between dream and favourite destinations},
