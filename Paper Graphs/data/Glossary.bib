	publisher = {PMLR},
	series = {Proceedings of Machine Learning Research},
	title = {{Reparameterization Gradients through Acceptance-Rejection Sampling Algorithms}},
	url = {https://proceedings.mlr.press/v54/naesseth17a.html},
