	location = {Torino, Italy},
	numpages = {4},
	pages = {1847--1850},
	publisher = {Association for Computing Machinery},
	series = {CIKM '18},
	title = {W2E: A Worldwide-Event Benchmark Dataset for Topic Detection and Tracking},
	url = {https://doi.org/10.1145/3269206.3269309},
	year = {2018},
