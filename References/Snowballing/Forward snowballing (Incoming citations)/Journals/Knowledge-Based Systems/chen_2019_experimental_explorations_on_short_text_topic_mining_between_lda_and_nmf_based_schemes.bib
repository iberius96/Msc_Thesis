	pages = {1--12},
	shorttitle = {{CDCN}},
	title = {{CDCN}: {A} {New} {NMF}-{Based} {Community} {Detection} {Method} with {Community} {Structures} and {Node} {Attributes}},
	url = {https://www.hindawi.com/journals/wcmc/2021/5517204/},
	urldate = {2022-11-25},
