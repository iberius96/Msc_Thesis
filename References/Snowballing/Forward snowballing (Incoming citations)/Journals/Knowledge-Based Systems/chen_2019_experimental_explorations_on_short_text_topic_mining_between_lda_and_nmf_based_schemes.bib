	issn = {2227-7390},
	journal = {Mathematics},
	language = {en},
	month = oct,
	number = {21},
	pages = {2722},
	title = {Topic-{Based} {Document}-{Level} {Sentiment} {Analysis} {Using} {Contextual} {Cues}},
