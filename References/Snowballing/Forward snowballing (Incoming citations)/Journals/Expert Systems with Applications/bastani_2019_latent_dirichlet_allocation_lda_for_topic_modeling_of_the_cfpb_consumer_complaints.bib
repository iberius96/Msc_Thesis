	language = {en},
	month = jul,
	pages = {1--13},
	title = {Fault {Diagnosis} of {Signal} {Equipment} on the {Lanzhou}-{Xinjiang} {High}-{Speed} {Railway} {Using} {Machine} {Learning} for {Natural} {Language} {Processing}},
